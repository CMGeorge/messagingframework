+template <class T>
+T IdFromUri(const QString& uri)
+{
+    int pos = uri.indexOf('#');
+    if (pos >= 0)
+    {
+        bool ok = false;
+        quint64 postfix = uri.right(uri.length() - pos-1).toULongLong(&ok);
+
+        if (ok)
+            return T(postfix);
+    }
+
+    return T();
+}
