+QString combineOperatorString(QMailKey::Combiner op)
+{
+    switch (op)
+    {
+    case And:
+        return " && ";
+        break;
+
+    case Or:
+        return " || ";
+        break;
+
+    case None:
+        break;
+    }
+
+    return QString();
+}
